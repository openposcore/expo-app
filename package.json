{
  "name": "openposcore-app",
  "main": "index.js",
  "version": "1.0.0",
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "prebuild": "expo prebuild",
    "run": "expo run:ios",
    "tunnel": "expo start -c --ios --tunnel",
    "dev": "expo start -c --ios",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@rn-primitives/avatar": "~1.0.4",
    "@rn-primitives/portal": "~1.0.3",
    "@rn-primitives/progress": "~1.0.3",
    "@rn-primitives/slot": "~1.0.3",
    "@rn-primitives/tooltip": "~1.0.3",
    "@rn-primitives/types": "~1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^51.0.24",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.7",
    "expo-router": "~3.5.20",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.0.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-keyboard-controller": "^1.13.4",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "run": "1.5.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.7",
    "expo-dev-client": "~4.0.26"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.79",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "private": true
}
